{
  "name": "hiruna",
  "version": "0.0.1",
  "description": "``` $ elm make src/Main.elm --output=main.js ```",
  "main": "index.js",
  "scripts": {
    "start": "elm reactor",
    "build-js": "elm make src/Main.elm --output=public/main.js",
    "build-css": "NODE_ENV=production tailwindcss-cli build style.css -o public/style.css",
    "build": "npm run build-js && npm run build-css",
    "format": "elm-format src/* --yes",
    "test": "elm-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xorphitus/hiruna"
  },
  "author": "xorphitus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xorphitus/hiruna/issues"
  },
  "homepage": "https://github.com/xorphitus/hiruna#readme",
  "devDependencies": {
    "@elm-tooling/elm-language-server": "^1.13.2",
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.4",
    "elm-test": "^0.19.1-revision4",
    "tailwindcss": "^2.0.2",
    "tailwindcss-cli": "^0.1.2"
  }
}
